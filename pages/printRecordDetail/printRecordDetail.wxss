/* pages/printRecordDetail/printRecordDetail.wxss */
page {
	font-size: 24rpx;
	color: rgb(51, 51, 51);
}
.printOrder {
	background: rgb(255, 255, 255);
  margin-bottom: 20rpx;
	padding: 15rpx 0rpx;
}
.printOrder .item {
  height: 54rpx;
  line-height: 54rpx;
	padding: 0rpx 30rpx;
}
.printOrder .item text {
	color: rgb(166, 166, 166);
}

.printRecord {
	background: rgb(255, 255, 255);
}
.printRecord .item {
  position: relative;
	background: rgb(255, 255, 255);
	padding: 40rpx 30rpx;
	border-bottom: 1rpx solid rgb(247, 247, 247);
}
.printRecord .item:last-child {
	border-bottom: 0rpx none;  
}
.item .name {
  display: inline-block;
  margin-right: 140rpx;
}
.item .name text {
	color: rgb(166, 166, 166);
}
.item .price {
  position: absolute;
  top: 40rpx;
  right: 30rpx;
	color: rgb(241, 92, 72);
}
.item .printAttr {
  display:block;
	margin-top: 50rpx;
	font-size: 22rpx;
	color: rgb(166, 166, 166);
}
.item .copyNum {
  position: absolute;
  bottom: 40rpx;
  right: 30rpx;
	font-size: 22rpx;
}